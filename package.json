{
  "author": "UIUX Lab",
  "email": "uiuxlab@gmail.com",
  "name": "uix-create-react-app",
  "projectName": "Uix Create React App",
  "createdInfo": "UIUX Lab (https://uiux.cc)",
  "projectURL": "https://uiux.cc",
  "version": "0.0.8",
  "description": "A set of React tool and scaffold.",
  "main": "webpack.config.js",
  "directories": {
    "example": "examples"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "^@react.app/components/(.*)": "<rootDir>/src/components/$1",
      "^@react.app/plugins/(.*)": "<rootDir>/src/components/_plugins/$1",
      "^@react.app/router/(.*)": "<rootDir>/src/router/$1",
      "^@react.app/pages/(.*)": "<rootDir>/src/views/_pages/$1"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.(ts|tsx)?$": "ts-jest"
    }
  },
  "scripts": {
    "watch": "webpack --watch --progress",
    "start:open": "webpack-dev-server --open-page http://localhost:8080/examples/",
    "dev": "cross-env NODE_ENV=development webpack --progress --mode development",
    "build": "cross-env NODE_ENV=production webpack --progress --mode production",
    "prettier": "prettier \"./dist/css/app.css\"",
    "test": "cross-env NODE_ENV=test jest"
  },
  "repository": {
    "type": "git",
    "url": "uix-create-react-app"
  },
  "keywords": [
    "web",
    "react",
    "app",
    "components",
    "ui",
    "front-end",
    "webdesign",
    "scaffold"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xizon/uix-create-react-app/issues"
  },
  "homepage": "https://github.com/xizon/uix-create-react-app#readme",
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@node-minify/core": "^6.2.0",
    "@node-minify/uglify-js": "^4.0.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^2.0.1",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "glslify-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.3.2",
    "identity-obj-proxy": "^3.0.0",
    "include-file-webpack-plugin": "^1.0.3",
    "jest": "^27.0.4",
    "jsdom": "^16.6.0",
    "json-loader": "^0.5.7",
    "log-timestamp": "^0.2.1",
    "mime-types": "^2.1.29",
    "mini-css-extract-plugin": "^2.2.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.2",
    "postcss": "^8.3.6",
    "prettier": "^1.19.1",
    "prettier-loader": "^2.1.1",
    "random-string": "^0.2.0",
    "raw-loader": "^4.0.2",
    "react-test-renderer": "^17.0.2",
    "safe-buffer": "^5.2.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tar": "^4.4.8",
    "terser-webpack-plugin": "^5.1.4",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "webpack": "^5.47.1",
    "webpack-cli": "^3.3.12",
    "webpack-concat-plugin": "^3.0.0",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "moment": "^2.23.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4,
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true
  },
  "eslintConfig": {
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "extends": [
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {},
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
}
